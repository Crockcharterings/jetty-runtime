<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <servlet>
    <servlet-name>asyncsessiontest</servlet-name>
    <servlet-class>com.google.apphosting.tests.usercode.testservlets.session.AsyncSessionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>dynproxyinsessiontest</servlet-name>
    <servlet-class>com.google.apphosting.tests.usercode.testservlets.session.DynProxyInSessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>asyncsessiontest</servlet-name>
    <url-pattern>/asyncsessiontest</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>dynproxyinsessiontest</servlet-name>
    <url-pattern>/dynproxyinSessiontest</url-pattern>
  </servlet-mapping>
</web-app>