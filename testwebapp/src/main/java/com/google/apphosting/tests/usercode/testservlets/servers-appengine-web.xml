<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>helloworld-java-maxr</application>
  <version>1</version>
  <!-- TODO(???) please update threadsafe to true if code is threadsafe -->
  <threadsafe>false</threadsafe>
  <servers>
    <!-- The tests assume this servers setup, do not change -->
    <backend name="hello"></backend>
    <backend name="memcache">
      <memory>500M</memory>
      <resident>true</resident>
      <instances>2</instances>
    </backend>
  </servers>
  <threadsafe>true</threadsafe>
</appengine-web-app>
