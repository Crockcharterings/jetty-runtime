<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
  <display-name>Test Servlets</display-name>

  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.google.apphosting.tests.usercode.testservlets.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>count</servlet-name>
    <servlet-class>com.google.apphosting.tests.usercode.testservlets.CountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>count</servlet-name>
    <url-pattern>/_ah/health</url-pattern>
  </servlet-mapping>
</web-app>
